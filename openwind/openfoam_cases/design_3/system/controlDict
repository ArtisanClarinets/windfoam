        startFrom       startTime;
        startTime       0;
        stopAt          endTime;
        endTime         500;
        deltaT          1;
        writeControl    timeStep;
        writeInterval   50;
        purgeWrite      0;
        writeFormat     ascii;
        writePrecision  6;
        runTimeModifiable yes;
        